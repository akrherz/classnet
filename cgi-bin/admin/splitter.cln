#!/usr/bin/perl
package main;

#
# Process Add/Drop requests from ADP
      open (MAPFILE, "</home/classnet/data/cm.dat") || die "Cannot open cm.dat.\n";
      open (OUTFILE, ">om.dat");
      my @data = <MAPFILE>;
      chomp @data;
      foreach $line (@data) {
        my @lines = split("F2001",$line);
        foreach $line (@lines) {
          if (length $line > 0) {
             print OUTFILE "F2001$line\n";
          }
        }
      }
      close MAPFILE;
      close OUTFILE;
